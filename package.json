{
  "name": "with-middleware",
  "version": "1.0.3",
  "description": "Add middleware support to any function prop or withState from recompose",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean": "rm -rf dist coverage index.js",
    "lint:js": "eslint \"src/**/**?(.spec|).js\"",
    "lint:format": "prettier --write \"{src,scripts}/**/**.js\"",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yeojz/with-middleware.git"
  },
  "keywords": [
    "react",
    "recompose",
    "hoc",
    "middleware",
    "withState",
    "dispatch",
    "store",
    "state",
    "withReducer",
    "withMiddleware",
    "setState"
  ],
  "author": "Gerald Yeo <contact@fusedthought.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yeojz/with-middleware/issues"
  },
  "homepage": "https://github.com/yeojz/with-middleware",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.4.2",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.2",
    "react-testing-library": "^5.0.0",
    "recompose": "^0.27.1",
    "rollup": "^0.64.1",
    "rollup-plugin-cleanup": "^3.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test-helpers"
    ],
    "resetMocks": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/test-helpers"
    ],
    "testURL": "http://localhost"
  }
}
